 LINKED LIST
class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

class LinkedList:
    def __init__(self):
        self.head = None

    def add(self, data):
        new_node = Node(data)
        if self.head is None:
            self.head = new_node
        else:
            temp = self.head
            while temp.next:
                temp = temp.next
            temp.next = new_node

    def show(self):
        temp = self.head
        while temp:
            print(temp.data, end=" => ")
            temp = temp.next
        print("None")

# Example
ll = LinkedList()
ll.add(10)
ll.add(20)
ll.add(30)
print("Linked List:")
ll.show()

                            STACK
class Stack:
    def __init__(self):
        self.items = []

    def push(self, data):
        self.items.append(data)

    def pop(self):
        return self.items.pop() if self.items else "Empty"

    def show(self):
        print("Stack:", self.items)

# Example
s = Stack()
s.push(10)
s.push(20)
s.push(30)
s.show()
print("Popped:", s.pop())
s.show()

                    
                           QUEUE
class Queue:
    def __init__(self):
        self.items = []

    def enqueue(self, data):
        self.items.append(data)

    def dequeue(self):
        return self.items.pop(2) if self.items else "Empty"

    def show(self):
        print("Queue:", self.items)

# Example
q = Queue()
q.enqueue(2)
q.enqueue(6)
q.enqueue(23)
q.show()
print("Dequeued:", q.dequeue())
q.show()

                        TREE(NODES)
class Node:
    def __init__(self, data):
        self.data = data
        self.left = None
        self.right = None

def inorder(root):
    if root:
        inorder(root.left)
        print(root.data, end=",")
        inorder(root.right)

# Example
root = Node(1)
root.left = Node(2)
root.right = Node(3)
root.left.left = Node(4)
root.left.right = Node(5)

print("Tree Inorder Traversal:")
inorder(root)
print()
